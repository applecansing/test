/* pages/add/add.wxss */
@import '../../css/zy_common.wxss';
.imgBox{
  position: fixed;
  width: 100%;
  height: 100%; 
  top: 0;
  left: 0;
}
.imgBox image{
  width: 100%;
  height: 100%;
}
.choseBox{
  position: fixed;
  left: 0;
  right: 0;
  top: 213rpx;
}
.choseBox >view {
   text-align: center;
}
 .choseBox >view navigator{
   display: inline-block;
} 
.choseBox image{
  width: 170rpx;
  height: 170rpx;
}
 .choseBox view:nth-child(1) navigator:not(:nth-child(3)){
  margin-right: 65rpx;
} 
 .choseBox view:nth-child(2) navigator:nth-child(1){
  margin-right: 65rpx;
} 
.choseBox>view:last-child{
  margin-top: 100rpx;
}
